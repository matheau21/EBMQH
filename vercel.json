{
  "buildCommand": "npx prisma generate && npm run build",
  "outputDirectory": "dist/spa",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/.*", "dest": "/api/[...path]" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
